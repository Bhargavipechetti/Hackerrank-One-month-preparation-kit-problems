/** Apple and Orange**/


def countApplesAndOranges(s, t, a, b, apples, oranges):
    num_of_apples = 0
    num_of_oranges = 0
    for i in apples:
        if s <= a + i <= t:
            num_of_apples += 1
    for j in oranges:
        if s <= b + j <= t:
            num_of_oranges += 1
    print(num_of_apples)
    print(num_of_oranges)
    
    
    
    


/**Grading Students**/


def gradingStudents(grades_list):
    rounded_grades=[]
    for i in grades_list:
        if i<38:
            rounded_grades.append(i)
        else:
            diff=5-(i%5)
            if diff<3:
                rounded_grades.append(i+diff)
            else:
                rounded_grades.append(i)
    return rounded_grades
